<h2>Articles Index</h2>
<table class="primary">
  <thead>
    <tr>
      <th>Description</th>
      <th>Notes</th>
      <th>Borrowed since</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each article in model}}
      <tr>
        <td>{{article.description}}</td>
        <td>{{article.notes}}</td>
        <td>{{article.createdAt}}</td>
        <td></td>
      </tr>
    {{/each}}
  </tbody>
</table>
